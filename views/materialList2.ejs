                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <!-- /.navbar-top-links -->
            <div class="navbar-default sidebar" role="navigation">
              <div class="title" style="padding:10px">
                <i class="teal huge user circle icon" style="display:inline-block"></i>
                <span><%=name%>님 환영합니다.</span>
