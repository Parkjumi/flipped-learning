                    <li><a href="/"><i class="fa fa-user fa-fw"></i> My 스터디</a>
                    </li>
                    <li><a href="/studysetting"><i class="fa fa-gear fa-fw"></i> Settings</a>
                    </li>
                    <li class="divider"></li>
                    <li><a href="/login"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                    </li>
                </ul> -->
              </div>
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="/"><i class="fa fa-dashboard fa-fw"></i> 내 정보</a>
                        </li>
                        <%if(first == name){%>
                          <li>
                              <a href="/studysetting/<%=title%>"><i class="fa fa-table fa-fw"></i> 스터디 관리</a>
                          </li>
                          <li>
                              <a href="#"><i class="fa fa-wrench fa-fw"></i> 자료 관리<span class="fa arrow"></span></a>
                              <ul class="nav nav-second-level">
                                    <li>
                                        <a href="/materialupload/<%=title%>">자료 등록</a>
                                    </li>

                                  <li>
                                      <a href="/materialList2/<%=title%>">자료 목록</a>
                                  </li>
                              </ul>
                              <!-- /.nav-second-level -->
                          </li>
                        <%}else{%>
                          <li>
                              <a href="#"><i class="fa fa-wrench fa-fw"></i>자료 목록</a>
                          </li>
                        <%}%>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper" style="padding-top:30px">
            <div class="row">
                <div style="padding-bottom:20px" class="col-lg-12">
                    <h1 class="page-header"><%=title%> - 자료 </h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
              <%if(material.length != 0){%>
                <%for(var i = 0;i < material.length; i++){%>
                  <form action="/materialList/<%=material[i].materialNum%>/<%=title%>" method="post">
                    <div class="col-lg-3 col-md-6">
                      <div style="border-color:#72A7A3;" class="panel panel-green">
                          <div style="background-color:#72A7A3;border-color:#72A7A3;" class="panel-heading">
                              <div class="row">
                                  <div class="col-xs-3">
                                      <i class="huge file alternate icon"></i>
                                  </div>
                                  <div class="col-xs-9 text-right">
                                      <span name="materialTitle"><%=i+1%> : <%=material[i].materialTitle%></span>
                                      <div><%=material[i].materialDate%></div>
                                  </div>
                              </div>
                          </div>
                          <button type="submit" style="margin: 0;padding: 0;width: 100%;border:0;outline:0;background-color:#F5F5F5">
                              <div class="panel-footer">
                                  <span style="color:#72A7A3;" class="pull-left">자료 상세 보기</span>
                                  <span class="pull-right"><i style="color:#72A7A3;" class="fa fa-arrow-circle-right"></i></span>
                                  <div class="clearfix"></div>
                              </div>
                          </button>
                      </div>
                    </div>
                  </form>
                  <%}%>
                <%}else{%>
                  <div class="col-lg-3 col-md-6">
                    <div style="border-color:#72A7A3;" class="panel panel-green">
                        <div style="background-color:#72A7A3;border-color:#72A7A3;" class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <i class="huge file alternate icon"></i>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div>자료를 등록해주세요</div>
                                    <div>자료를 등록해주세요</div>
                                </div>
                            </div>
                        </div>
                        <button href="#" type="submit" style="margin: 0;padding: 0;width: 100%;border:0;outline:0;background-color:#F5F5F5" >
                            <div class="panel-footer">
                                <span style="color:#72A7A3;" class="pull-left">자료 상세 보기</span>
                                <span class="pull-right"><i style="color:#72A7A3;" class="fa fa-arrow-circle-right"></i></span>
                                <div class="clearfix"></div>
                            </div>
                        </button>
                    </div>
                  </div>
                <%}%>
