    <div class="form" >
      <form class="ui form" method="post" action="/studysetting/<%=studyTitle%>">
        <h2 class="ui dividing header">스터디 그룹 설정</h2>
        <div class="field">
          <label>스터디 명</label>
          <div class="disabled two fields">
            <div class="field">
              <input type="text" name="studyTitle" value="<%=data.studyTitle%>" placeholder="스터디 명을 입력하세요.">
            </div>
          </div>
        </div>
        <div class="field">
          <label>스터디 주제</label>
          <div class="disabled two fields">
            <div class="field">
              <input type="text" name="studyTopic" value="<%=data.studyTopic%>" placeholder="스터디 주제를 입력하세요.">
            </div>
          </div>
        </div>
        <div class="field">
          <label>역할 기간</label>
          <div class="three fields">
            <div class="field">
              <input type="number" name="role" value="<%=data.role%>" placeholder="역할기간을 입력하세요." aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <div class="field">
          <label>활동 기간</label>
          <div class="three fields">
            <div class="field">
              <input id="myDate" type="DATE" name="active" value="<%=data.active%>"  placeholder="활동기간을 입력하세요." aria-describedby="basic-addon1">

            </div>
          </div>
        </div>
        <div class="field">
          <label>교사 학습 기간</label>
          <div class="three fields">
            <div class="field">
              <input type="number" name="teacher" value="<%=data.teacher%>" placeholder="교사 학습기간을 입력하세요." aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <div class="field">
          <label>최대 인원</label>
          <div class="disabled three fields">
            <div class="field">
              <input type="number" name="max" value="<%=data.max%>" placeholder="최대 인원을 입력하세요." aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <div class="field">
          <label>모임 장소</label>
          <div class="two fields">
            <div class="field">
              <input type="text" name="meet" value="<%=data.meet%>" placeholder="주 모임 장소를 입력하세요." aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <div class="field">
          <label>내용</label>
          <div class="two fields">
            <div class="field">
              <input type="text" name="content" value="<%=data.content%>" placeholder="내용을 입력하세요." aria-describedby="basic-addon1">
            </div>
          </div>
        </div>
        <!-- <div class="ui button" tabindex="0" type="submit">등록하기</div> -->
        <br>
        <button class="ui teal basic button" type="submit">수정하기</button>

